{
  "name": "venom",
  "version": "0.1.0",
  "description": "Venom is an UI KIT with MadeiraMadeira brand",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "sass-build-autoprefix": "postcss --use autoprefixer --map false --output dist/venom.css dist/venom.css",
    "sass-build-cleancss": "cleancss -o dist/venom.min.css dist/venom.css",
    "sass-build-clean": "rimraf css",
    "sass-build-sass": "node-sass --output-style expanded --source-map true venom.sass dist/venom.css",
    "deploy": "npm run sass-build-clean && npm run sass-build-sass && npm run sass-build-autoprefix && npm run sass-build-cleancss && gulp",
    "start": "npm run sass-build-sass -- --watch",
    "linter": "sass-lint -c .sass-lint.yml 'sass/**/*.sass' -v -q",
    "prepublish": "tsc --strictNullChecks"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.4.0",
    "@typescript-eslint/parser": "^2.4.0",
    "autoprefixer": "^9.6.1",
    "clean-css-cli": "^4.3.0",
    "cssnano": "^4.1.10",
    "eslint": "^6.5.1",
    "gulp": "^4.0.2",
    "gulp-replace": "^1.0.0",
    "node-sass": "^4.12.0",
    "postcss-cli": "^6.1.2",
    "postcss-easy-import": "^3.0.0",
    "rimraf": "^2.6.3",
    "sass-lint": "^1.13.1",
    "tailwindcss": "^1.0.5",
    "typescript": "^3.6.4",
    "vanilla-masker": "^1.2.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/madeiramadeirabe/venom.git"
  },
  "keywords": [
    "sass",
    "css",
    "html",
    "ui",
    "javascript"
  ],
  "author": "William Novak",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/madeiramadeirabe/venom/issues"
  },
  "homepage": "https://github.com/madeiramadeirabe/venom#readme",
  "dependencies": {},
  "engines": {
    "node": ">=6.0.0"
  },
  "files": [
    "dist/index.js",
    "dist/index.d.ts",
    "dist/venom.js",
    "dist/venom.d.ts",
    "dist/types.js",
    "dist/types.d.ts",
    "dist/components",
    "dist/utils",
    "dist/venom.css",
    "dist/icons",
    "dist/venom.min.css"
  ]
}
