{
  "name": "venom",
  "version": "0.1.0",
  "description": "Venom is an UI KIT with MadeiraMadeira brand",
  "main": "index.js",
  "scripts": {
    "sass-build-autoprefix": "postcss --use autoprefixer --map false --output css/venom.css css/venom.css",
    "sass-build-cleancss": "cleancss -o css/venom.min.css css/venom.css",
    "sass-build-clean": "rimraf css",
    "sass-build-sass": "node-sass --output-style expanded --source-map true venom.sass css/venom.css",
    "sass-deploy": "npm run sass-build-clean && npm run sass-build-sass && npm run sass-build-autoprefix && npm run sass-build-cleancss",
    "sass-start": "npm run sass-build-sass -- --watch",
    "sass-linter": "sass-lint -c .sass-lint.yml 'src/sass/**/*.sass' -v -q",
    "site-dev": "next",
    "site-deploy": "next build",
    "site-start": "next start"
  },
  "devDependencies": {
    "autoprefixer": "^9.6.1",
    "clean-css-cli": "^4.3.0",
    "cssnano": "^4.1.10",
    "node-sass": "^4.12.0",
    "postcss-cli": "^6.1.2",
    "postcss-easy-import": "^3.0.0",
    "rimraf": "^2.6.3",
    "sass-lint": "^1.13.1",
    "tailwindcss": "^1.0.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/madeiramadeirabe/venom.git"
  },
  "keywords": [
    "sass",
    "css",
    "html",
    "ui",
    "javascript"
  ],
  "author": "William Novak",
  "contributors": [
    {
      "name": "Celso Fabri Junior",
      "email": "celso.fabri@gmail.com",
      "url": "https://github.com/celsofabri"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/madeiramadeirabe/venom/issues"
  },
  "homepage": "https://github.com/madeiramadeirabe/venom#readme",
  "dependencies": {
    "@zeit/next-css": "^1.0.1",
    "next": "^9.0.2",
    "normalize.css": "^8.0.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  }
}
