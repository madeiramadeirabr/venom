{
  "name": "venom",
  "version": "0.1.0",
  "description": "Venom is an UI KIT with MadeiraMadeira brand",
  "main": "index.js",
  "scripts": {
    "sass-build-autoprefix": "postcss --use autoprefixer --map false --output css/venom.css css/venom.css",
    "sass-build-cleancss": "cleancss -o css/venom.min.css css/venom.css",
    "sass-build-clean": "rimraf css",
    "sass-build-sass": "node-sass --output-style expanded --source-map true venom.sass css/venom.css",
    "deploy": "npm run sass-build-clean && npm run sass-build-sass && npm run sass-build-autoprefix && npm run sass-build-cleancss && gulp",
    "start": "npm run sass-build-sass -- --watch",
    "linter": "sass-lint -c .sass-lint.yml 'src/sass/**/*.sass' -v -q"
  },
  "devDependencies": {
    "autoprefixer": "^9.6.1",
    "clean-css-cli": "^4.3.0",
    "cssnano": "^4.1.10",
    "gulp": "^4.0.2",
    "node-sass": "^4.12.0",
    "postcss-cli": "^6.1.2",
    "postcss-easy-import": "^3.0.0",
    "rimraf": "^2.6.3",
    "sass-lint": "^1.13.1",
    "tailwindcss": "^1.0.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/madeiramadeirabe/venom.git"
  },
  "keywords": [
    "sass",
    "css",
    "html",
    "ui",
    "javascript"
  ],
  "author": "William Novak",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/madeiramadeirabe/venom/issues"
  },
  "homepage": "https://github.com/madeiramadeirabe/venom#readme",
  "dependencies": {}
}